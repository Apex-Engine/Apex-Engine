void libapex_toolkit_default_initunittools()
{
    libapex_toolkit_setmaxlevel(20);
    libapex_toolkit_setlevelbehavior("Levels");
    libapex_toolkit_setexperiencegiverbehavior("GiveExperience");
	
	
    libapex_toolkit_setdamagebehavior("IncreaseDamage1",1);
	libapex_toolkit_setdamagebehavior("IncreaseDamage10",2);
	libapex_toolkit_setdamagebehavior("IncreaseDamage100",3);
	libapex_toolkit_setdamagebehavior("IncreaseDamage1000",4);
	
	libapex_toolkit_setdamagebehavior("DecreaseDamage1",5);
	libapex_toolkit_setdamagebehavior("DecreaseDamage10",6);
	libapex_toolkit_setdamagebehavior("DecreaseDamage100",7);
	libapex_toolkit_setdamagebehavior("DecreaseDamage1000",8);

	libapex_toolkit_setrangebehavior("IncreaseRange1",1);
	libapex_toolkit_setrangebehavior("IncreaseRange10",2);
	
	libapex_toolkit_setrangebehavior("DecreaseRange1",4);
	libapex_toolkit_setrangebehavior("DecreaseRange10",5);

	libapex_toolkit_setarmorbehavior("IncreaseLifeArmorP1",1);
	libapex_toolkit_setarmorbehavior("IncreaseLifeArmor1",2);
	libapex_toolkit_setarmorbehavior("IncreaseLifeArmor10",3);
	libapex_toolkit_setarmorbehavior("IncreaseShieldArmorP1",4);
	libapex_toolkit_setarmorbehavior("IncreaseShieldArmor1",5);
	libapex_toolkit_setarmorbehavior("IncreaseShieldArmor10",6);
	libapex_toolkit_setarmorbehavior("DecreaseLifeArmorP1",7);
	libapex_toolkit_setarmorbehavior("DecreaseLifeArmor1",8);
	libapex_toolkit_setarmorbehavior("DecreaseLifeArmor10",9);
	libapex_toolkit_setarmorbehavior("DecreaseShieldArmorP1",10);
	libapex_toolkit_setarmorbehavior("DecreaseShieldArmor1",11);
	libapex_toolkit_setarmorbehavior("DecreaseShieldArmor10",12);

	libapex_toolkit_setperiodbehavior("SetAttackSpeed0P9", 0);
	libapex_toolkit_setperiodbehavior("SetAttackSpeed1P1", 1);
}

void libapex_toolkit_default_initrestrictedunits()
{
	libapex_toolkit_addrestrictedunit("Spawner");
	libapex_toolkit_addrestrictedunit("nukenoodle");
}
